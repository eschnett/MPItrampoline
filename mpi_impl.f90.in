subroutine mpitrampoline_mpi_init()

  use mpi
  
  implicit none

#define CONSTANT(TYPE, NAME) \
  [n]integer WPI_##NAME \
  [n]common /MPI_##NAME/ WPI_##NAME
#include "mpi-constants-f.inc"
#undef CONSTANT

#define CONSTANT(TYPE, NAME) \
  MPI_##NAME = WPI_##NAME
#include "mpi-constants-f.inc"
#undef CONSTANT

end subroutine mpitrampoline_mpi_init
